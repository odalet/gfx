<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <StaticResource x:Key="ThemeBackgroundBrush" ResourceKey="SystemRegionBrush" />
            <StaticResource x:Key="ThemeForegroundBrush" ResourceKey="SystemControlForegroundBaseHighBrush" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <StaticResource x:Key="ThemeBackgroundBrush" ResourceKey="SystemRegionBrush" />
            <StaticResource x:Key="ThemeForegroundBrush" ResourceKey="SystemControlForegroundBaseHighBrush" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
