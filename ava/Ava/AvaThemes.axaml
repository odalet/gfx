<Styles x:Class="Ava.AvaThemes"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ava="using:Ava"
        xmlns:palettes="using:Ava.Themes.Palettes">
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceInclude x:Key="Default" Source="/Themes/Light.axaml" />
                <ResourceInclude x:Key="Dark" Source="/Themes/Dark.axaml" />
                <ResourceInclude x:Key="{x:Static ava:AvaThemes.HighContrast}" Source="/Themes/HighContrast.axaml" />
            </ResourceDictionary.ThemeDictionaries>
            <ResourceDictionary.MergedDictionaries>
                <!-- <ResourceInclude Source="/Themes/Palettes/BasePalette.axaml" /> -->
                <!-- <palettes:SystemAccentColors /> -->
                <!-- <palettes:PaletteResourcesCollection /> -->
                <!-- <ResourceInclude Source="/Themes/Palettes/BaseResources.axaml" /> -->
                <!-- <ResourceInclude Source="Themes/Shared/all.axaml" /> -->
                <ResourceInclude Source="Themes/Controls/all.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Styles.Resources>
    <!-- <StyleInclude Source="/Styles/Styles.axaml" /> -->
</Styles>