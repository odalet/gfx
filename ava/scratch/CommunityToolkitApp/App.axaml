<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="CommunityToolkitApp.App"
             xmlns:local="using:CommunityToolkitApp"
             xmlns:semi="https://irihi.tech/semi"
             xmlns:ava="https://odalet/ava"
             RequestedThemeVariant="Default">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>
    <Application.Resources>
        <ResourceDictionary>
            <ava:FluentTheme x:Key="FluentTheme" />
            <ava:SimpleTheme x:Key="SimpleTheme" />
            <StyleInclude x:Key="AvaControls" Source="avares://Ava/Styles/Controls.axaml" />
        </ResourceDictionary>
    </Application.Resources>
    <Application.Styles>

    </Application.Styles>
</Application>